            <Label alignment="CENTER" text="Update My Profile">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <HBox alignment="CENTER" prefHeight="34.0" prefWidth="439.0" spacing="10.0">
               <children>
                  <Label prefHeight="18.0" prefWidth="122.0" text="Username" />
                  <TextField fx:id="usernameField" prefHeight="26.0" prefWidth="230.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" layoutX="11.0" layoutY="72.0" prefHeight="34.0" prefWidth="439.0" spacing="10.0">
               <children>
                  <Label prefHeight="18.0" prefWidth="122.0" text="Name" />
                  <TextField fx:id="nameFeild" prefHeight="26.0" prefWidth="230.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" layoutX="11.0" layoutY="131.0" prefHeight="34.0" prefWidth="439.0" spacing="10.0">
               <children>
                  <Label prefHeight="18.0" prefWidth="122.0" text="Phone" />
                  <TextField fx:id="phoneFeild" prefHeight="26.0" prefWidth="230.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" layoutX="11.0" layoutY="164.0" prefHeight="34.0" prefWidth="439.0" spacing="10.0">
               <children>
                  <Label prefHeight="18.0" prefWidth="122.0" text="Address" />
                  <TextField fx:id="addressField" prefHeight="26.0" prefWidth="230.0" />
               </children>
            </HBox>
            <Button mnemonicParsing="false" onAction="#updateProfile" prefHeight="47.0" prefWidth="101.0" text="Update" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </content>
</DialogPane>
